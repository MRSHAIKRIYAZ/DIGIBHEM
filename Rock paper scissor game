import random as r
while True:
 a=["rock","scissor","paper"]
 b=input("enter your choice:-")
 c=r.choice(a)
 print(c)
 if c==b:
	 print("match draw")
 elif c=="rock "and b== "scissor":
	 print("rock wins")
 elif c=="rock" and b=="paper":
	 print("paper wins")
 elif c=="paper" and b=="rock":
	 print("paper wins")
 elif c=="paper" and b=="scissor":
	 print("scissor wins")
 elif c=="scissor" and b==" rock":
	 print("scissor wins")
 elif c=="scissor" and b=="paper":
	 print("paper wins")
